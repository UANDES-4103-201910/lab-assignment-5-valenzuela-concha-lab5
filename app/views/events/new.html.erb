
<h1>New Event</h1>


<%= form_with scope: :event, url: events_path, local: true do |form| %>
  <p>
    <%= form.label :Name %><br>
    <%= form.text_field :name %>
  </p>
 
  <p>
    <%= form.label :Description %><br>
    <%= form.text_area :description %>
  </p>

  <p>
    <%= form.label :EventVenue %><br>
    <%= form.text_area :event_venue_id %>
  </p>

  <p>
    <%= form.label :StartDate %><br>
    <%= form.text_area :start_date %>
  </p>
 
  <p>
    <%= form.submit %>
  </p>
<% end %>

<%= link_to 'Back', events_path %>

